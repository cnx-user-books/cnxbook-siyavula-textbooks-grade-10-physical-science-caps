                </m:mtr>
                <m:mtr>
                  <m:mtd columnalign="right">
                    <m:msub>
                      <m:mi>v</m:mi>
                      <m:mi>f</m:mi>
                    </m:msub>
                  </m:mtd>
                  <m:mtd>
